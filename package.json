{
  "name": "gym-client-manager",
  "version": "1.0.0",
  "private": true,
  "main": "public/electron.js",
  "author": "Ridel Saavedra Flores",
  "description": "AplicaciÃ³n",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/material": "^5.15.9",
    "@mui/x-data-grid": "^6.19.4",
    "@react-pdf/renderer": "^3.4.4",
    "axios": "^1.6.7",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.0.5",
    "formik": "^2.4.5",
    "framer-motion": "^11.0.3",
    "i18next": "^22.4.9",
    "query-string": "^8.2.0",
    "react": "^18.2.0",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.5",
    "sass": "^1.53.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack-build": "npm run build && electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "electron-pack-deploy": "npm run build && electron-builder build --win -c.extraMetadata.main=build/electron.js --publish always",
    "preelectron-pack": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "electron": "^18.3.7",
    "electron-builder": "^22.14.5",
    "wait-on": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shakarr/gymclientmanager"
  },
  "build": {
    "appId": "com.ridelsaavedraflores.gymclientmanager",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "build/favicon.ico",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "installerIcon": "build/favicon.ico"
    }
  }
}
